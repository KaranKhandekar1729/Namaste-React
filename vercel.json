{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "builds": [
        {
            "src": "index.html",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}